<?xml version="1.0" encoding="UTF-8"?>
<redis>
<application>
	<id>ephub-product-business-service</id>
	<cluster>
		<clusterid>default</clusterid>
		<configuration>
			<!-- 最大的活动连接 -->
			<maxtotal>2000</maxtotal>
			<!-- 最大的空闲连接 -->
			<maxidle>100</maxidle>
			<!-- 最大的等待时间 -->
			<maxwait>1500</maxwait>
			<!-- 过期时间 -->
			<expiretime>60000</expiretime>
			<!-- 默认超时时间 -->
			<defaulttimeout>1500</defaulttimeout>
			<!-- testonborrow -->
			<testonborrow>true</testonborrow>
			<!-- testonreturn -->
			<testonreturn>true</testonreturn>
			<!-- 密码 -->
			<password>123456</password>
		</configuration>
		<redisnodes>
			<redisnode>
				<!--节点编号 -->
				<id>default-node-one</id>
				<!--Ip地址和端口 -->
				<host>172.20.30.56</host>
				<port>9501</port>
			</redisnode>
			<redisnode>
				<!--节点编号 -->
				<id>default-node-two</id>
				<!--Ip地址和端口 -->
